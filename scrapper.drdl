      SqlName: _id
      SqlType: objectid
    - Name: 8 500 - 12 500 PLN gross/month - Permanent
      MongoType: string
      SqlName: 8 500 - 12 500 PLN gross/month - Permanent
      SqlType: varchar
    - Name: address
      MongoType: string
      SqlName: address
      SqlType: varchar
    - Name: author
      MongoType: string
      SqlName: author
      SqlType: varchar
    - Name: Brand Story
      MongoType: string
      SqlName: Brand Story
      SqlType: varchar
    - Name: city
      MongoType: string
      SqlName: city
      SqlType: varchar
    - Name: Company name
      MongoType: string
      SqlName: Company name
      SqlType: varchar
    - Name: Company size
      MongoType: string
      SqlName: Company size
      SqlType: varchar
    - Name: date
      MongoType: string
      SqlName: date
      SqlType: varchar
    - Name: description
      MongoType: string
