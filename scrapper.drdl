    - Name: "ul \u0141ucka 2/4/6 lok u4, WarszawaFully Remote"
      MongoType: string
      SqlName: "ul \u0141ucka 2/4/6 lok u4, WarszawaFully Remote"
      SqlType: varchar
  - table: offers_salary
    collection: offers
    pipeline:
    - $unwind:
        includeArrayIndex: salary_idx
        path: $salary
        preserveNullAndEmptyArrays: false
    columns:
    - Name: _id
      MongoType: bson.ObjectId
      SqlName: _id
      SqlType: objectid
    - Name: salary.average
      MongoType: string
      SqlName: salary.average
      SqlType: varchar
    - Name: salary.average_pln
      MongoType: string
      SqlName: salary.average_pln
      SqlType: varchar
    - Name: salary.currency
      MongoType: string
      SqlName: salary.currency
      SqlType: varchar
    - Name: salary.employment_type
      MongoType: string
      SqlName: salary.employment_type
      SqlType: varchar
    - Name: salary.lower_range
      MongoType: string
      SqlName: salary.lower_range
      SqlType: varchar
    - Name: salary.lower_range_pln
      MongoType: string
      SqlName: salary.lower_range_pln
      SqlType: varchar
    - Name: salary.upper_range
      MongoType: string
      SqlName: salary.upper_range
      SqlType: varchar
    - Name: salary.upper_range_pln
      MongoType: string
      SqlName: salary.upper_range_pln
      SqlType: varchar
    - Name: salary_idx
      MongoType: int
      SqlName: salary_idx
      SqlType: int
  - table: offers_skills
    collection: offers
    pipeline:
    - $unwind:
        includeArrayIndex: skills_idx
        path: $skills
        preserveNullAndEmptyArrays: false
    columns:
    - Name: _id
      MongoType: bson.ObjectId
      SqlName: _id
      SqlType: objectid
    - Name: skills.skill_level
      MongoType: string
      SqlName: skills.skill_level
      SqlType: varchar
    - Name: skills.skill_name
      MongoType: string
      SqlName: skills.skill_name
      SqlType: varchar
    - Name: skills_idx
      MongoType: int
      SqlName: skills_idx
      SqlType: int
